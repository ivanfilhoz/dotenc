{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run dotenc Extension (Monorepo)",
			"type": "extensionHost",
			"request": "launch",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}/vscode-extension"]
		},
		{
			"name": "Run dotenc Extension (Folder)",
			"type": "extensionHost",
			"request": "launch",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}"]
		}
	]
}
